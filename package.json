{
  "name": "epjs",
  "version": "1.0.0",
  "description": "",
  "main": "epjs.js",
  "scripts": {
    "preinstall": "cd ui && npm install && cd ..",
    "clean": "gulp clean",
    "build": "gulp scripts",
    "prerelease": "gulp scripts && cd ui && npm run release && cd .. && npm run lint",
    "release": "npm run pkg-backend",
    "pkg-windows": "pkg package.json --target node10-win-x64 --output release/epjs.exe",
    "pkg-linux": "pkg package.json --target node10-linux-x64 --output release/epjs",
    "pkg-mac": "pkg package.json --target node10-mac-x64 --output release/epjs.app",
    "pkg-backend": "npm run pkg-windows && npm run pkg-linux",
    "start": "npm run build && node ./dist/epjs.js dev",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^11.11.3",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.1",
    "gulp-clean": "^0.4.0",
    "typescript": "^3.3.3333",
    "pkg": "^4.3.7",
    "tslint": "^5.14.0"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "event-stream": "^4.0.1",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.3-alpha.1",
    "mongodb": "^3.1.13",
    "unzip": "^0.1.11"
  },
  "bin": "dist/epjs.js",
  "pkg": {
    "assets": [
      "ui/dist/ui/**/*"
    ],
    "scripts": [
      "ui/dist/ui/**/*.js"
    ]
  }
}
