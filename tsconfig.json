{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "sourceMap": true,
    "outDir": "./dist/",
    "rootDir": "./src/",
    "strict": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
  },
  "exclude": [
    "node_modules",
    "ui"
  ]
}