{
  "name": "epjs-backend",
  "version": "1.0.0",
  "description": "",
  "main": "epjs.js",
  "scripts": {
    "build": "gulp scripts",
    "pkg-windows": "pkg dist/epjs.js --target node10-win-x64 --output release/epjs.exe",
    "pkg-linux": "pkg dist/epjs.js --target node10-linux-x64 --output release/epjs",
    "pkg-mac": "pkg dist/epjs.js --target node10-mac-x64 --output release/epjs.app",
    "pkg-backend": "npm run pkg-windows && npm run pkg-linux",
    "release": "gulp scripts && npm run pkg-backend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^11.11.3",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.1",
    "typescript": "^3.3.3333",
    "pkg": "^4.3.7"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.3-alpha.1",
    "mongodb": "^3.1.13",
    "rxjs": "^6.4.0",
    "uuid": "^3.3.2"
  }
}
